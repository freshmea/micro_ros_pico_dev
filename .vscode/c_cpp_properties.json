{
  "configurations": [
    {
      "name": "Pico",
      "includePath": [
        "${workspaceFolder}/micro_ros_pico_dev/src/**",
        "${workspaceFolder}/micro_ros_pico_dev/libmicroros/include/**",
        "${workspaceFolder}/micro_ros_pico_dev/libfixmath/**",
        "${workspaceFolder}/pico-sdk/src/**",
        "${workspaceFolder}/pico-sdk/lib/**",
        "/usr/lib/gcc/arm-none-eabi/*/include",
        "/usr/include/newlib/c++/**"
      ],
      "defines": [
        "PICO_BOARD=pico2_w",
        "PICO_RP2350=1",
        "PICO_CYW43_ARCH_THREADSAFE_BACKGROUND=1",
        "LIB_PICO_CYW43_ARCH=1",
        "CYW43_LWIP=1",
        "PICO_STDIO_USB=1",
        "PICO_STDIO_UART=0"
      ],
      "compilerPath": "/usr/bin/arm-none-eabi-gcc",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "intelliSenseMode": "gcc-arm",
      "compilerArgs": [
        "-mcpu=cortex-m33",
        "-mthumb",
        "-mfloat-abi=soft"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}/micro_ros_pico_dev/src",
          "${workspaceFolder}/micro_ros_pico_dev/libmicroros/include",
          "${workspaceFolder}/micro_ros_pico_dev/libfixmath",
          "${workspaceFolder}/pico-sdk/src"
        ],
        "limitSymbolsToIncludedHeaders": true,
        "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
      }
    }
  ],
  "version": 4
}